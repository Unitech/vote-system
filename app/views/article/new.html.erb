<div class="new-article">
  <center><h1><%= t 'Post a new article' %></h1></center>
  <br/>
  <div id="middle_pos">
    <%= form_for @article, :url => :create_article do |f| %>
      <%= render 'form', :f => f %>
      <center><%= f.submit t 'Submit article' %></center>
    <% end %>
  </div>
</div>
